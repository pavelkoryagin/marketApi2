  const observedTargetIdsMapedToObservers = new Map();
  function mountNodeHandler(event) {
    const {
      detail: {
        showUrl,
        pixel,
        render,
        isDirectTGOBanner,
        parentImpression,
        targetId,
        directParentId,
        isHeroBannerSeparated,
        markAsBilled,
