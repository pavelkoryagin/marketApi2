    let timeout = null;
    const intersectionObserver = new IntersectionObserver(entries => {
      var _entries$;
      if ((_entries$ = entries[0]) !== null && _entries$ !== void 0 && _entries$.isIntersecting) {
        if (!timeout) {
          timeout = setTimeout(() => {
            if (showUrl) {
              fetch(`${madvStaticPath}${encodeURIComponent(showUrl)}`, {
                method: 'GET',
                keepalive: true,
                credentials: 'include',
                referrerPolicy: 'no-referrer-when-downgrade'
              });
            }
            if (pixel) {
              fetch(pixel, {
                method: 'GET',
                credentials: isDirectTGOBanner ? 'include' : undefined,
                mode: 'no-cors'
              });
            }
            if (directParentId && parentImpression && !parentIdSet.has(directParentId)) {
              parentIdSet.add(directParentId);
              if (parentImpression !== null && parentImpression !== void 0 && parentImpression.length) {
                parentImpression.forEach(impression => {
                  fetch(impression, {
                    method: 'GET',
                    credentials: 'include'
                  });
                });
              }
            }
            if (markAsBilled) {
              markAsBilled();
            }
            intersectionObserver.disconnect();
          }, 1000);
        }
      } else {
        timeout && clearTimeout(timeout);
        timeout = null;
      }
    }, {
      threshold: 0.5,
      ...observerOptions
    });
    if (target && !isRegistratedInObserver) {
      intersectionObserver.observe(target);
      observedTargetIdsMapedToObservers.set(targetId, intersectionObserver);
      if (markAsRegistratedInObserverHandler) markAsRegistratedInObserverHandler();
    }
    if (targetIdToDisconnect) {
      const observer = observedTargetIdsMapedToObservers.get(targetIdToDisconnect);
      if (observer) {
        observer.disconnect();
        observedTargetIdsMapedToObservers.delete(targetIdToDisconnect);
      }
    }
  }
