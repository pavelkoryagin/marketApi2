    if (render !== null && render !== void 0 && render.length) {
      render.forEach(r => {
        fetch(r, {
          method: 'GET',
          credentials: 'include'
        });
      });
    }
    const target = targetId && document.getElementById(targetId);
    const parentIdSet = new Set();
    const observerOptions = {};
    if (target && isHeroBannerSeparated) {
      observerOptions.rootMargin = '0% -30%';
      observerOptions.root = isLazyHeroSeparatedExp ? document.querySelector('[data-apiary-widget-name="@marketfront/HeroBannerCarouselLazyCreator"]') : document.querySelector('[data-apiary-widget-name="@marketfront/HeroBannerCarouselCreator"]');
